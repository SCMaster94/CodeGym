   "source": [
    "import turtle\n",
    "def draw_rec(width, height, color):\n",
    "    t=turtle.Turtle()\n",
    "    t.color(color)\n",
    "    t.forward(width)\n",
    "    t.right(90)\n",
    "    t.forward(height)\n",
    "    t.right(90)\n",
    "    t.forward(width)\n",
    "    t.right(90)\n",
    "    t.forward(height)\n",
    "    t.right(90)\n",
    "    turtle.done()\n",
    "draw_rec(50,100,'red')\n"
   ]
  },
